{
    "id": "teknikio-bluebird",
    "name": "bluebird",
    "corepkg": "teknikio-bluebird",
    "title": "Teknikio Bluebird - Blocks / Javascript Editor",
    "description": "A Blocks / Javascript code editor for the Teknikio Bluebird",
    "bundleddirs": [
        "libs/base",
        "libs/core",
        "libs/buttons",
        "libs/accelerometer",
        "libs/music",
        "libs/mixer",
        "libs/mixer---samd",
        "libs/lightsensor",
        "libs/color",
        "libs/light",
        "libs/touch",
        "libs/switch",
        "libs/pulse",
        "libs/cable",
        "libs/infrared",
        "libs/mouse",
        "libs/keyboard",
        "libs/gamepad",
        "libs/servo",
        "libs/storage",
        "libs/serial",
        "libs/jacdac",
        "libs/teknikio-bluebird",
        "libs/core---nrf52832"
    ],
    "serial": {
      "useHF2": true,
      "useEditor": true,
      "log": true,
      "editorTheme": {
        "graphBackground": "#d9d9d9",
        "lineColors": [
          "#009DDC",
          "#FB48C7",
          "#40bf4a",
          "#8073E5",
          "#d25b33"
        ]
      }
    },
    "variants": {
      "nrf52832": {
        "serial": {
          "useHF2": false
        },
        "compile": {
          "openocdScript": "source [find interface/cmsis-dap.cfg]; source [find target/nrf52.cfg]",
          "flashChecksumAddr": 0,
          "deployFileMarker": "MBED.HTM",
          "vtableShift": 3,
          "useUF2": false,
          "webUSB": false,
          "gc": true
        },
        "compileService": {
          "codalTarget": {
            "name": "codal-ble-nano",
            "url": "https://github.com/lancaster-university/codal-ble-nano",
            "branch": "v0.0.3-jacdac-v0.0",
            "type": "git"
          },
          "codalBinary": "BLE_NANO",
          "serviceId": "codal2ble",
          "dockerImage": "pext/yotta:latest",
          "yottaConfig": {
            "pxt": {
              "board": {
                "id": "BOARD_ID_NRF52832"
              }
            }
          }
        }
      }
    },
    "runtime": {
        "mathBlocks": true,
        "loopsBlocks": true,
        "logicBlocks": true,
        "variablesBlocks": true,
        "textBlocks": true,
        "listsBlocks": true,
        "functionBlocks": true,
        "functionsOptions": {
          "useNewFunctions": true
        },
        "onStartNamespace": "loops",
        "onStartColor": "#40bf4a",
        "onStartWeight": 100,
        "bannedCategories": [
          "jacdac"
        ]
    },
    "simulator": {
        "autoRun": true,
        "streams": false,
        "aspectRatio": 1.0,
        "parts": true,
        "partsAspectRatio": 0.69,
        "dynamicBoardDefinition": true
    },
    "cloud": {
      "workspace": false,
      "packages": true,
      "sharing": true,
      "publishing": true,
      "importing": true,
      "thumbnails": true,
      "preferredPackages": [],
      "githubPackages": true
    },
    "appTheme": {
        "accentColor": "#0089BF",
        "logoUrl": "https://github.com/Teknikio/pxt-teknikio",
        "logo": "./static/logo.png",
        "organization": "Teknikio",
        "organizationUrl": "https://www.teknikio.com/",
        "organizationLogo": "https://cdn.shopify.com/s/files/1/1837/7355/files/logo_larger_600x.png?v=1572991143",
        "organizationWideLogo": "https://cdn.shopify.com/s/files/1/1837/7355/files/logo_larger_600x.png?v=1572991143",
        "homeUrl": "https://www.teknikio.com/pages/bluebird",
        "embedUrl": "https://www.teknikio.com/pages/bluebird",
        "privacyUrl": "https://www.teknikio.com/pages/privacy-policy",
        "termsOfUseUrl": "https://www.teknikio.com/pages/terms-and-conditions",
        "githubUrl": "https://github.com/Teknikio/pxt-teknikio",
        "boardName": "Teknikio Bluebird",
        "docMenu": [
            {
                "name": "About",
                "path": "/about"
            },
            {
                "name": "Docs",
                "path": "/docs"
            },
            {
              "name": "FAQ",
              "path": "https://www.teknikio.com/pages/faq"
            }
        ],
        "coloredToolbox": false,
        "monacoToolbox": true,
        "invertedMenu": true,
        "invertedToolbox": true,
        "invertedMonaco": true,
        "selectLanguage": true,
        "showHomeScreen": true,
        "homeScreenHero": "./static/hero.png",
        "blocklyOptions": {
            "grid": {
                "spacing": 20,
                "length": 2,
                "colour": "rgba(189, 195, 199, 0.20)",
                "snap": true
            }
        },
        "blocklyColors": {
          "insertionMarker": "#FFFFFF",
          "insertionMarkerOpacity": 0.5
        },
        "blockColors": {
          "loops": "#40bf4a",
          "logic": "#4cbfe6",
          "math": "#9966ff",
          "variables": "#FF6680",
          "text": "#ffab19",
          "advanced": "#00272B",
          "functions": "#005a9e",
          "arrays": "#8A1C7C"
        },
        "allowPackageExtensions": true,
        "alwaysGithubItem": true,
        "monacoColors": {
            "editor.background": "#ecf0f1"
        },
        "socialOptions": {
          "orgTwitterHandle": "teknikio"
        },
        "simAnimationEnter": "roll in",
        "simAnimationExit": "roll out",
        "debugger": true,
        "highContrast": true,
        "greenScreen": true,
        "enableTrace": true,
        "print": true,
        "hasAudio": true,
        "extendEditor": true,
        "extendFieldEditors": true,
        "disableBlockIcons": true,
        "scriptManager": true,
        "baseTheme": "dark",
        "qrCode": true
    },
    "unsupportedBrowsers": [
      { "id" : "ie" }
    ]
}